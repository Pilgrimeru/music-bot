{
  "name": "music-bot",
  "version": "2.7.0",
  "description": "EvoBot fork: Improved music experience, bug fixes, multiple sources.",
  "main": "index.ts",
  "author": "Pilgrimeru",
  "private": true,
  "homepage": "https://github.com/Pilgrimeru/music-bot",
  "repository": "github:Pilgrimeru/music-bot",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
