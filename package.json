  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "array-move": "3.0.1",
    "axios": "^1.4.0",
    "discord.js": "^14.10.0",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "i18n": "^0.15.1",
    "isomorphic-unfetch": "^4.0.2",
    "lyrics-finder": "^21.7.0",
    "music-metadata": "^7.13.4",
    "play-dl": "^1.9.6",
    "spotify-url-info": "^3.2.3",
    "string-progressbar": "^1.0.4",
    "systeminformation": "^5.17.12",
    "ts-node": "^10.9.1",
    "youtube-sr": "^4.3.4"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.6",
    "@types/node": "^20.1.4",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "libsodium-wrappers": "^0.7.11",
    "sodium-native": "^4.0.1",
    "tweetnacl": "^1.0.3"
  },
  "overrides": {
    "make-dir": "^4.0.0",
    "semver": "^7.5.2"
  },
  "config": {
    "commitizen": {
